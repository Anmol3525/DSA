#include <stdio.h>

#define MAX 100

int main() {
    int m, n;
    int mat1[MAX][MAX], mat2[MAX][MAX], result[MAX][MAX];

    scanf("%d %d", &m, &n);

    if (m > MAX || n > MAX)
        return 0;

    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &mat1[i][j]);
        }
    }

    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            scanf("%d", &mat2[i][j]);
        }
    }

    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            result[i][j] = mat1[i][j] + mat2[i][j];
        }
    }

    for (int i = 0; i < m; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d", result[i][j]);
            if (j != n - 1)
                printf(" ");
        }
        printf("\n");
    }

    return 0;
}
